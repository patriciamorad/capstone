{
  "name": "Mesh",
  "description": "Add events to Mesh",
  "version": "0.7.3",
  "permissions": ["management", "tabs", "activeTab", "<all_urls>"],

  "content_security_policy":
    "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": "meshlogo.png",
    "default_title": "App Launcher",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "meshlogo.png"
  },
  "manifest_version": 2
}
